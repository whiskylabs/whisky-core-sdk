{
  "name": "@whisky-gaming/core",
  "private": false,
  "version": "1.0.1",
  "description": "Core SDK for Whisky Gaming Protocol",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whisky-gaming/whisky-core.git"
  },
  "homepage": "https://whisky.gg",
  "bugs": {
    "url": "https://github.com/whisky-gaming/whisky-core/issues"
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch --format cjs,esm --dts",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc --noEmit",
    "test": "jest",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.27.0",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.93.0",
    "bn.js": "^5.2.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.1",
    "@types/jest": "^29.5.5",
    "eslint": "^8.48.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": "^0.27.0",
    "@solana/web3.js": "^1.93.0"
  },
  "keywords": [
    "solana",
    "gaming",
    "blockchain",
    "casino",
    "defi",
    "whisky",
    "protocol",
    "sdk",
    "web3",
    "smart-contracts"
  ],
  "author": "Sarthak Kapila",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
} 